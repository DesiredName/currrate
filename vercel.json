{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=120, stale-while-revalidate"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=120"
        }
      ]
    }
  ]
}